{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JWT": {
    "Secret": "${JWT_SECRET}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "ExpirationMinutes": 10
  },
  "Cors": {
    "AllowedOrigins": [
      "https://estatehub.com",
      "https://www.estatehub.com",
      "https://app.estatehub.com"
    ],
    "AllowLocalhost": true
  }
}
