{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "PLACEHOLDER - Use environment variable DB_CONNECTION_STRING"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "/var/log/estatehub/listing-service/app-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 30,
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] [{SourceContext}] {Message:lj}{NewLine}{Exception}",
          "shared": true
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "JWT": {
    "Secret": "PLACEHOLDER - Use environment variable JWT_SECRET",
    "Issuer": "PLACEHOLDER - Use environment variable JWT_ISSUER",
    "Audience": "PLACEHOLDER - Use environment variable JWT_AUDIENCE"
  },
  "MongoDB": {
    "ConnectionString": "PLACEHOLDER - Use environment variable MONGODB_CONNECTION_STRING",
    "DatabaseName": "PLACEHOLDER - Use environment variable MONGODB_DATABASE",
    "GridFSBucket": "photos"
  },
  "AuthService": {
    "GrpcUrl": "http://auth-service:8082"
  },
  "Cors": {
    "AllowedOrigins": [
      "PLACEHOLDER - Use environment variables CORS_ORIGIN_1, CORS_ORIGIN_2, etc."
    ],
    "AllowLocalhost": false
  },
  "OpenAI": {
    "ApiKey": "PLACEHOLDER - Use environment variable OPENAI_API_KEY",
    "Model": "gpt-3.5-turbo"
  },
  "Moderation": {
    "Enabled": true,
    "FailOpen": false
  },
  "EnforceHttpsRedirect": true
}

